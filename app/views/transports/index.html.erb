<p id="notice"><%= notice %></p>

<h1>Transports</h1>

<table>
  <thead>
    <tr>
      <th>Fecha</th>
      <th>Tipo</th>
      <th>Hora</th>
      <th>Dia</th>
      <th>Detalles</th>
      <th>Longitude</th>
      <th>Latitude</th>
      <th>Address</th>
      <th>Tipo transp</th>
      <th>Images</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @transports.each do |transport| %>
      <tr>
        <td><%= transport.fecha %></td>
        <td><%= transport.tipo %></td>
        <td><%= transport.hora %></td>
        <td><%= transport.dia %></td>
        <td><%= transport.detalles %></td>
        <td><%= transport.longitude %></td>
        <td><%= transport.latitude %></td>
        <td><%= transport.address %></td>
        <td><%= transport.tipo_transp %></td>
        <td><%= transport.images %></td>
        <td><%= link_to 'Show', transport %></td>
        <td><%= link_to 'Edit', edit_transport_path(transport) %></td>
        <td><%= link_to 'Destroy', transport, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Transport', new_transport_path %>
