<head>
  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1"/>
  <title>Safe & Sound</title>
  <!--<h1>Safe & Sound</h1>-->
  <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
  <!-- Materialize -->
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js"></script>
  <script src="js/init.js"></script>

  <!-- CSS  -->
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
</head>

<div id="index-banner" class="parallax-container" style="line-height: 1">
  <div class="section no-pad-bot">
    <div class="container">
      <h1 class="header center" style="color: #455a64 ">Log in</h1>
      <div class="row">
        <div class="col s12 m4 offset-m4 l4 offset-l4" style="color: #455a64">
          <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
            <div class="input-field" >
              <%= f.label :email, {:style => "color: #455a64"} %><br />
              <%= f.email_field :email, autofocus: true %>
            </div>

            <div class="input-field">
              <%= f.label :password, {:style => "color: #455a64"} %><br />
              <%= f.password_field :password, autocomplete: "off" %>
            </div>

            <% if devise_mapping.rememberable? -%>
              <div class="field">
                <%= f.check_box :remember_me %>
                <%= f.label :remember_me,{:style => "color: #455a64"} %>
              </div>
            <% end -%>

            <div class="actions btn-large waves-effect waves-light blue-grey darken-1" style="margin: 10px">
              <%= f.submit "Log in" %>
            </div>
          <% end %>
          <%= render "users/shared/links" %>
        </div>
      </div>
    </div>
  </div>
  <div class="parallax"><img src="<%= asset_path('mapa.png') %>" alt=""></div>
</div>
