$('#suspects').append('<%= j render @suspects %>');
<% if @suspects.next_page %>  
  $('.pagination').replaceWith('<%= j will_paginate @suspects%>');
<% else %>  
  $(window).off('scroll');
  $('.pagination').remove();
<% end %> 

